  <!-- Example 2: Time-Limited License (Expires on specific date) -->
  <License Key="AZSL-AnotherKeyHere" User="Jane Smith" HWID="XYZ98765MNOP4321" Expiry="2025-12-31" />
  
  <!-- Add more licenses below... -->
  
</Licenses>
